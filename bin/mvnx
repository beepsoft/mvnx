#!/usr/bin/env node

const mvnx = require('../src/index.js')
const BaseMvnxError = require('../src/lib/error/BaseMvnxError');

(async function main() {
    try {
        await mvnx.run(process.argv)
    } catch (err) {
        if (err instanceof BaseMvnxError) {
            console.error();
            console.error(err.toString());

            process.exit(1);
        }
    }
})()
